    <file src="..\remainingCharacters.min.js" target="content\Scripts\k4gdw\remainingCharacters.min.js" />
    <file src="..\remainingCharacters.intellisense.js" target="content\Scripts\k4gdw\remainingCharacters.intellisense.js" />
    <file src="..\README.md" target="content\App_Readme\k4gdw\remainingCharacters\README.md" />
  </files>
</package>
